{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "cmake build",
      "type": "shell",
      "command": "cmake --build build",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "cmake clean",
      "type": "shell",
      "command": "cmake --build build --target clean"
    }
  ]
}
